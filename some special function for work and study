     //Array移动
     swapItems:function(arr, index1, index2) {
                arr[index1] = arr.splice(index2, 1, arr[index1])[0];
                return arr;
            },
      // 上移
      up:function (index) {
          this.swapItems(arr, index, index - 1);
      },
      // 下移
      down:function (index) {
          this.swapItems(arr, index, index + 1);
      },
